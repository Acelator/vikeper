  "paths": {
    "/{apiV}/tasks/{tokenId}": {
      "get": {
        "description": "Returns all the tasks that belongs to the owner of the tokenId",
        "parameters": [
          {
            "$ref": "#components/parameters/tasks"
          },
          {
            "$ref": "#components/parameters/token"
          }
        ],
        "responses": {
          "200": {
            "description": "List of tasks",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#components/schemas/task"
                  }
                }
              }
            }
          },
          "404": {
            "description": "tokenId doesn't belong to any user",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#components/schemas/error"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/{apiV}/tasks/{tokenId}/new": {
      "get": {
        "parameters": [
          {
          }
        ]
      }
    },
    "/{apiV}/tasks/{tokenId}/task/{taskId}": {
      "get": {
        "parameters": []
      },
      "post": {
        "parameters": []
      }
    },
    "/{apiV}/tasks/{tokenId}/task/{taskId}/delete": {
      "get": {
        "parameters": []
      }
    }
